{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/core": "8.8.4",
        "composer/installers": "^1.2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/simple_oauth": "^3",
        "drupal/metatag": "1.9",
        "drupal/migrate_tools": "4.3",
        "drupal/twig_tweak": "2.4",
        "drupal/devel": "2.1",
        "drupal/devel_php": "1.1",
        "drupal/mailgun": "1.0-rc1",
        "mailgun/mailgun-php": "2.8.1",
        "drupal/viewsreference":"2.0.0-alpha4",
        "drupal/views_field_view":"1.0-beta2",
        "drupal/views_ajax_history":"1.2",
        "drupal/twig_vardumper":"1.2.0",
        "drupal/twig_field_value":"1.2.0",
        "drupal/title_field_for_manage_display":"1.9.0",
        "drupal/smart_trim":"1.1.0",
        "drupal/simple_sitemap":"3.3.0",
        "drupal/shield":"1.2.0",
        "drupal/search_api_solr":"1.2.0",
        "drupal/search_api_exclude_entity":"1.0.0",
        "drupal/search_api":"1.14",
        "drupal/scheduler":"1.0.0",
        "drupal/redirect":"1.4.0",
        "drupal/recaptcha":"2.4.0",
        "drupal/purge_drush":"3.0-beta9",
        "drupal/purge":"3.0.0-beta9",
        "drupal/private_files_download_permission":"2.0.0",
        "drupal/pathauto":"1.6.0",
        "drupal/paragraphs":"1.6.0",
        "drupal/minifyhtml":"1.8.0",
        "drupal/migrate_source_csv":"2.2.0",
        "drupal/menu_link_attributes":"1.0.0",
        "drupal/memcache":"2.0",
        "drupal/linkit":"5.0.0-beta7",
        "drupal/leaflet":"1.22",
        "drupal/inline_entity_form":"1.0-rc2",
        "drupal/honeypot":"1.29.0",
        "drupal/google_tag":"1.2",
        "drupal/geolocation_google_maps":"2.0.0-beta1",
        "drupal/geolocation":"2.0.0-beta1",
        "drupal/geofield_map":"2.20.0",
        "drupal/geofield":"1.0.0",
        "drupal/file_browser":"1.1.0",
        "drupal/field_group":"3.0.0-beta1",
        "drupal/features":"3.8",
        "drupal/facets":"1.4.0",
        "drupal/extlink":"1.1.0",
        "drupal/eu_cookie_compliance":"1.5.0",
        "drupal/entity_reference_revisions":"1.6.0",
        "drupal/entity_embed":"1.0.0",
        "drupal/entity_browser":"2.0.0-alpha3",
        "drupal/embed":"1.0.0",
        "drupal/dropzonejs_eb_widget":"2.0.0-alpha4",
        "drupal/dropzonejs":"2.0.0-alpha4",
        "drupal/dfp":"1.0-alpha1",
        "drupal/ctools":"3.2.0",
        "drupal/config_update":"1.5.0",
        "drupal/cog":"1.10.0",
        "drupal/coder":"8.3.6",
        "drupal/chosen_lib":"2.7.0",
        "drupal/chosen":"2.7.0",
        "drupal/captcha":"1.0-beta4",
        "drupal/better_exposed_filters":"3.0-alpha6",
        "drupal/ajax_loader":"1.4.0",
        "drupal/adminimal_admin_toolbar":"1.7.0",
        "drupal/admin_toolbar":"2.0.0-beta1",
        "drupal/address":"1.4.0",
        "drupal/acquia_purge":"1.1-beta1",
        "drupal/acquia_connector":"1.16.0",
        "drupal/migrate_plus":"4.2",
        "drupal/userprotect": "1.0",
        "drush/drush": "10.2.0",
        "drupal/custom_sql_migrate_source_plugin": "v1.0",
        "drupal/unpublished_nodes_redirect": "1.1",
        "cweagans/composer-patches": "^1.6",
        "drupal/hacked": "2.0-beta2",
        "drupal/diff": "1.0-rc2",
        "drupal/colorbox": "1.4",
        "drupal/video_embed_field": "2.2",
        "drupal/video_embed_jwplayer": "1.0-rc2",
        "drupal/console":"1.9.3",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/structure_sync": "^1.16",
        "drupal/webform": "^5.9.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
